Algoritmo "MultiplosDe3"

Var
  x1, x2: INTEIRO;

Inicio
  escreva("Digite o número:")
  leia(x1)
  escreva("Digite o número:")
  leia(x2)
  
  se(x1%3) E (x2%3)=0 entao
      escreva("Os números escolhidos são múlltiplos de 3")
  senao
  se (x1<>3) E (x2<>3)=0 entao
      escreva("Os números escolhidos não são múltiplos de 3")
  fimse
  fimse
  
   
Fimalgoritmo