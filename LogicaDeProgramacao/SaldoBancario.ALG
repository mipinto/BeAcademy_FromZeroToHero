Algoritmo "SaldoBancario"

Var
   valorInicial: REAL;
   depositoInicial: REAL;
Inicio
    escreva("Informe o valor inicial em conta:" )
    leia(valorInicial)
    
    escreva("Informe o valor depositado em conta:" )
    leia(depositoInicial)
    
    escreva("O saldo atual da conta é:", valorInicial + depositoInicial)



Fimalgoritmo